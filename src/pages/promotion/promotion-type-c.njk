{% block js %}
<script src="/asset/js/promotion.js"></script>
<script>
  var productList = [];
  function swierInit(element, isLoop) {
    var instance = $(element).data('swiper');
    if(instance) {
      instance.update();
      return false;
    }

    var swiper = new Swiper(element, {
      loop: isLoop,
      pagination: {
        el: '.swiper-pagination'
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      },
      on: {
        init: function () {
          if(isLoop){
            swiperPagination(this);
          }
        },
        slideChange: function () {
          if(isLoop){
            swiperPagination(this);
          }
        },
        resize: function(){
          // throttle
          this.update();
        },
        update: function(){
          if(isLoop){
            swiperPagination(this);
          }
        }
      }
    });

    if(isLoop){
      $(element).addClass('is-active');
    } else {
      swiper.detachEvents();
    }
    $(element).data('swiper', swiper);
    productList.push(swiper);
  }

  function swiperPagination(instance) {
    var swiper = instance;
    var slides = swiper.slides;
    var length = swiper.params.loop
      ? slides.length - 2
      : slides.length;
    var percent = (swiper.realIndex + 1) / length * 100;
    window.setTimeout(function () {
      $(swiper.pagination.el)
        .parent()
        .find('.swiper-progress__fill')
        .width(percent + '%');
    }, 10)
  }

  $(function () {
    // 기획전 상단 슬라이드 이미지
    new Swiper('#topPromotionSwiper', {
      loop: true,
      slidesPerView: 'auto',
      breakpoints: {
        1023: {
        loop: false,
        slidesPerView: 1,
        }
      },
      pagination: {
        el: '.swiper-pagination',
        type: 'fraction'
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      },
      on: {
        init: function (swiper) {
          var swiper = this;
          var length = swiper.slides.length;
          if (swiper.activeIndex !== swiper.realIndex) {
            length = length - 2;
          }
          var percent = (swiper.realIndex + 1) / length * 100;
          window.setTimeout(function () {
            $(swiper.pagination.el)
              .parent()
              .find('.swiper-progress__fill')
              .width(percent + '%');
          }, 10)
        },
        touchStart: function (swiper, event) {
          $(this.el).addClass('is-move');
        },
        touchEnd: function (swiper, event) {
          $(this.el).removeClass('is-move');
        },
        progress: function () {
          var swiper = this;
          var length = swiper.slides.length;
          var percent = swiper.progress / (length - 2) * (length - 1) * 100;

          if (swiper.activeIndex === swiper.realIndex) {
            percent = swiper.progress * 100;
          }

          if (percent > 100) {
            percent = percent - 100;
          } else if (percent <= 0) {
            percent = 100;
          }
          $(swiper.pagination.el)
            .parent()
            .find('.swiper-progress__fill')
            .width(percent + '%')
        }
      }
    })

    // ctype 상품 슬라이드
    new Swiper('#ctypeProductSwiper', {
      loop: false,
      freeMode: true,
      spaceBetween: 30,
      slidesOffsetBefore: 175,
      slidesPerView: 'auto',
      breakpoints: {
        1023: {
          loop: true,
          spaceBetween: 20,
          slidesOffsetBefore: 0,
          slidesPerView: 1.5,
          centeredSlides: true
        }
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      }
    })

    // PICK 이미지 슬라이드
    var pickImageSlides = $('#pickImageSwiper .swiper-slide');
    new Swiper('#pickImageSwiper', {
      loop: pickImageSlides.length > 1,
      pagination: {
        el: '.swiper-pagination',
        type: 'fraction'
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      }
    })
    
    // PICK 상품 슬라이드
    new Swiper('#pickProductSwiper', {
      loop: false,
      freeMode: true,
      centerInsufficientSlides: true,
      spaceBetween: 20,
      slidesPerView: 'auto',
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      }
    })


    // 상품 슬라이더
    $('#productList .swiper-container').each(function () {
    var $element = $(this);
    // 동적으로 추가되는 상품도 아래와 같이 추가필요함
    swierInit($element[0], $element.find('.swiper-slide').length > 1);
    });

    // 필터 카테고리 트리메뉴
    $(document).on('click', '.category-filter .button--toggle', function(){
    var category = $(this).closest('.category-filter__list-item');
    var subMenu = category.find('> .category-filter__list');
    if(category.hasClass('extend')){
      category.removeClass('extend');
      subMenu.slideUp();
    } else {
      category.addClass('extend');
      subMenu.slideDown();
    }
    });

    // 필터 관련 UI (선택된 필터)
    // filter.add(FilterInfo) // 필터 선택
    // filter.remove(FilterInfo) // 필터 삭제
    // filter.reset() // 필터 전부 삭제
    /**
    * @typedef {Object} FilterInfo
    * @property {string} key // 필터 구분 ex) gender, color, size ...
    * @property {string} value // 필터 값 ex) F,M, BLK, 4455 ... 
    * @property {string} name // 필터 화면 노출 이름 ex) 남, 여, BLACK, 44 - 55
    */
    $(document)
    // 예제
    .on('change', '.option-list--gender input[type=checkbox]', function(){
      // UI확인용 Sample (성별) - 확인용 이므로 개발시 동일하게 사용 불가능함
      //window.filter.add / window.filter.remove 사용 방법 참고용
      if(!$(this).prop('checked')){
        console.log('삭제', '성별', $(this).val())
        window.filter.remove({key: 'gender', value: $(this).val()});
      } else {
        console.log('추가', '성별', $(this).val());
        window.filter.add({
          key: 'gender', value: $(this).val(), name: $(this).data('name')
        });
      }
    })
    // 삭제
    .on('click', '[data-filter-remove]', function(e){
      e.preventDefault();
      const item = $(e.target).closest('[data-filter]');
      const key = item.data('filter');
      const value = item.data('filterValue');
      window.filter.remove({key: key, value: value});
      // UI확인용 Sample (성별) - 확인용 이므로 개발시 동일하게 사용 불가능함
      // 선택된 필터에서 X클릭 -> 필터 > 체크박스 동기화 참고용
      $('.option-list--gender input[type=checkbox]').each(function(){
        if($(this).val() === value) {
          console.log('체크해제', '성별', $(this).val());
          $(this).prop('checked', false);
        }
      })
    })
      // 초기화
    .on('click', '[data-filter-reset]', function(e){
      // UI확인용 Sample (성별) - 확인용 이므로 개발시 동일하게 사용 불가능함
      // window.filter.reset(); 참고용
      e.preventDefault();
      window.filter.reset();
      $('.option-list--gender input[type=checkbox]').each(function(){
        if($(this).prop('checked')) {
          console.log('체크해제', '성별', $(this).val());
          $(this).prop('checked', false);
        }
      })
    })
    // 필터 레이어 Open
    .on('click', '#filterOpenButton', function(e){
      e.preventDefault();
      ui.modules.sidebar.show();
    });
  });
</script>
